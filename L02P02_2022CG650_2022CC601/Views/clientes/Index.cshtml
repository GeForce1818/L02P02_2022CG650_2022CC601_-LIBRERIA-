@{
}
<h1>Proceso de Venta</h1>
<br />
<form>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Email</th>
                <th>Dirección</th>
                <th>Creado</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var clientes in (IEnumerable<dynamic>)ViewData["listadoDeClientes"])
            {
                <tr>
                    <td>@clientes.nombre</td>
                    <td>@clientes.apellido</td>
                    <td>@clientes.email</td>
                    <td>@clientes.direccion</td>
                    <td>@clientes.created_at</td>

                </tr>
            }
        </tbody>
    </table>

</form>
